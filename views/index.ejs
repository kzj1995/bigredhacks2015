    <section id="connect" class="home-section text-center">
		<div class="heading-contact">
			<div class="container">
			<img src="img/leaf-yellow.svg" draggable="false" style="float:left;"><br><br><h2 style="float:left;">Connect</h2><br>
				</div>
			</div>
